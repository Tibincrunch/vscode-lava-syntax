{
	"name": "language-lava",
	"displayName": "Lava Languages Support",
	"description": "Lava Languages Syntax Highlighter",
	"version": "0.0.2",
	"publisher": "GarrettJohnson",
	"engines": {
		"vscode": "^1.16.0"
	},
	"categories": [
		"Programming Languages"
	],
	"icon": "images/rockrms-icon.jpg",
	"galleryBanner": {
		"color": "#161821",
		"theme": "dark"
	},
	"preview": true,
	"keywords": [
		"rockrms",
		"lava",
		"grammar",
		"church"
	],
	"homepage": "https://github.com/garrettjohnson/vscode-lava-syntax",
	"repository": {
		"type": "git",
		"url": "https://github.com/garrettjohnson/vscode-lava-syntax.git"
	},
	"bugs": "https://github.com/garrettjohnson/vscode-lava-syntax",
	"contributes": {
		"languages": [
			{
				"id": "lava",
				"aliases": [
					"Lava (HTML)",
					"lava"
				],
				"extensions": [
					".lava"
				],
				"configuration": "./lava.language-configuration.json"
			},
			{
				"id": "xml",
				"aliases": ["Master (Rock Theme)"],
				"extensions": [".master"]
			}
		],
		"grammars": [
			{
				"language": "lava",
				"scopeName": "text.html.lava",
				"path": "./syntaxes/lava.json"
			}
		]
	}
}